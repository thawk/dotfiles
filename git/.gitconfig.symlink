# Local/private config goes in the include
[include]
        path = ~/.gitconfig.local
[push]
        default = simple
[core]
        eol = lf
        autocrlf = false
        filemode = false

[alias]
        co = checkout
        ci = commit
        st = status -s
        br = branch
        hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
        type = cat-file -t
        dump = cat-file -p
